{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "waterfogsw/docker-frontend",
            "hostname": "frontend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "frontend",
            "image": "waterfogsw/docker-backend",
            "hostname": "backend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "waterfogsw/docker-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "linkes": ["frontend", "backend"],
            "memory": 128
        }

    ]
}